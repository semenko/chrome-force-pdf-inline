{
    "author": "Nick Semenkovich",
    "description": "Encourage PDFs to show in the browser instead of downloading. By Nick Semenkovich <semenko@alum.mit.edu>",
    "manifest_version": 3,
    "background": {
        "service_worker": "service_worker.js"
    },
    "name": "Render PDFs Inline",
    "permissions": [
        "declarativeNetRequest", "declarativeNetRequestFeedback"
    ],
    "host_permissions": [
	"<all_urls>"
    ],
    "declarative_net_request": {
        "rule_resources": [{
          "id": "ruleset",
          "enabled": true,
          "path": "ruleset.json"
        }]
      },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "version": "2024.10.20"
}
